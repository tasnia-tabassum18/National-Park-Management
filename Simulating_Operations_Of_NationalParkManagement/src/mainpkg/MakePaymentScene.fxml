<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.MakePaymentSceneController">
   <children>
      <Label layoutX="98.0" layoutY="121.0" text="Enter account number:">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="120.0" layoutY="173.0" text="Enter Password:">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="179.0" layoutY="28.0" style="-fx-background-color: white;" text="Please fill out the informations">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="accountnumberTextField" layoutX="311.0" layoutY="116.0" onMouseClicked="#accountnumberTextFieldOnMouseClick" />
      <TextField fx:id="passwordTextField" layoutX="311.0" layoutY="168.0" onMouseClicked="#passwordTextFieldOnMouseClick" />
      <Button layoutX="300.0" layoutY="227.0" mnemonicParsing="false" onAction="#confirmPaymentButtonOnClick" prefHeight="27.0" prefWidth="224.0" style="-fx-background-color: orange; -fx-border-color: black;" text="Confim Payment">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
